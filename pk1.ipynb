        "import pandas as pd\n",
        "import seaborn as sns\n",
        "\n",
        "# 加载数据集\n",
        "data = sns.load_dataset('titanic')\n",
        "\n",
        "# 选择'embarked'作为要编码的列（表示乘客登船的地点）\n",
        "one_hot_encoded = pd.get_dummies(data['embarked'], prefix='embarked')\n",
        "\n",
        "print(one_hot_encoded.head())"
